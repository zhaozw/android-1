<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.jitsi"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:name=".android.gui.Jitsi"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".android.gui.account.AccountsList"
                    android:label="@string/app_name">
        </activity>
        <activity android:name=".android.gui.account.NewAccount"
                    android:label="@string/app_name">
        </activity>
        <activity android:name=".android.gui.call.CallContact"
                    android:label="@string/app_name">
        </activity>
        <activity android:name=".android.gui.call.VideoCall"
                    android:label="@string/app_name">
        </activity>
        <service android:name=".service.osgi.OSGiService" />
    </application>
    <uses-feature android:glEsVersion="0x00020000"/>
    <uses-feature android:name="android.hardware.sip.voip" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-sdk android:minSdkVersion="11" />
</manifest>
